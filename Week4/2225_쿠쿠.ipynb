   "source": [
    "n, k = map(int, input().split())\n",
    "s = [[0] * 201 for i in range(201)]\n",
    "\n",
    "for i in range(201):\n",
    "    s[i][1] = 1\n",
    "\n",
    "for i in range(1, 201):\n",
    "    for j in range(201):\n",
    "        for l in range(j + 1):\n",
    "            s[j][i] += s[l][i - 1]\n",
    "\n",
    "print(s[n][k] % 1000000000)"
   ]
  }
